<ITEM><GAP DESC="foreign">, ra&s;h.</ITEM>
<ITEM><GAP DESC="foreign">, new.</ITEM>
<ITEM><GAP DESC="foreign">, gra&s;&s;e.</ITEM>
<ITEM><GAP DESC="foreign">, an Orchard.</ITEM>
<ITEM><GAP DESC="foreign">, to creake.</ITEM>
<ITEM><GAP DESC="foreign">, a &s;tarre.</ITEM>
<ITEM><GAP DESC="foreign">, whole.</ITEM>
<ITEM><GAP DESC="foreign">, foule.</ITEM>
<ITEM><GAP DESC="foreign">, a Dere.</ITEM>
<ITEM><GAP DESC="foreign">, a rodde.</ITEM>
<ITEM><GAP DESC="foreign">, re&s;t.</ITEM>
<ITEM><GAP DESC="foreign">, the Moone.</ITEM>
<ITEM><GAP DESC="foreign">, a mill.</ITEM>
<ITEM><GAP DESC="foreign">, a treate.</ITEM>
<ITEM><GAP DESC="foreign">, a &s;hippe.</ITEM>
